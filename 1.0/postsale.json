          {
            "name": "orderId",
            "in": "query",
            "description": "The relevant order ID",
            "type": "string",
            "format": "uuid"
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "$ref": "#/definitions/CancellationOptions"
            }
          }
        },
        "security": [
          {
            "token": [
              "token"
            ]
          }
        ]
      }
    },
    "/api/PostSale/CreateCancellation": {
      "post": {
        "tags": [
          "PostSale"
        ],
        "summary": "CreateCancellation",
